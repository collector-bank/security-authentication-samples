@model System.Security.Claims.ClaimsPrincipal

@{
    ViewBag.Title = "Protected Page";
}

<h2>Protected Page</h2>
Protected Page

<table>
    <tr>
        <th>Type</th>
        <th>Value</th>
        <th>Issuer</th>
        <th>Original Issuer</th>
    </tr>

    @foreach (var claim in Model.Claims)
    {
        <tr><td>@claim.Type</td><td>@claim.Value</td><td>@claim.Issuer</td><td>@claim.OriginalIssuer</td></tr>
    }
</table>